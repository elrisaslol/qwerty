<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ComboBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.AddTropaController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      
      <TextField fx:id="name" promptText="NAME" />
       <ComboBox fx:id="combo_ids" prefWidth="150.0" promptText="COMPANYS" />

       <Button mnemonicParsing="false" onAction="#closeWindow" text="Agregar" />
   </children>
</VBox>
