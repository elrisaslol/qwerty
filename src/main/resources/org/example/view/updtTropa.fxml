<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.UpdateTropasController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
       <ComboBox fx:id="combo_ids" prefWidth="150.0" promptText="UNIT" />
       <TextField fx:id="name" promptText="NEW NAME" />
       <ComboBox fx:id="combo_NEWids" prefWidth="150.0" promptText="NEW COMPANY(OPT)" />

      <Button mnemonicParsing="false" onAction="#closeWindow" text="Modificar" />

   </children>
</VBox>
